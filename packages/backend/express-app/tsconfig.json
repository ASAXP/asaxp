{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "paths": {
      "@asaxp/types": ["../../libs/types/src"]
    },
    "allowJs": true,
    "outDir": "./dist",
    "composite": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "skipLibCheck": true
  },
  "references": [{ "path": "../../libs/types" }]
}
